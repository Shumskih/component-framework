{
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "platform": {
      "php": "7.3.0"
    }
  },
  "require": {
    "php": ">=7.1.0",
    "ext-mbstring": "*",
    "psr/http-message": "^1.0",
    "roave/security-advisories": "dev-master",
    "zendframework/zend-diactoros": "^2.2",
    "zendframework/zend-httphandlerrunner": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Framework\\": "src/Framework/",
      "Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}
